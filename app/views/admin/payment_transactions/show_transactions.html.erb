<%
  @heading = 'Payment Transactions'
  @sub_heading = 'All Transaction Details'
  content_for :content do %>
    <div class="scrollbar">
      <table class="table table-striped table-hover" id="payment-transactions-table" data-source="<%= admin_show_transactions_path(@user, format: :json) %>">
        <thead>
        <tr>
          <th>ID</th>
          <th>Amount</th>
          <th>Status</th>
          <th>Mobile Number</th>
          <th>Type</th>
          <th>Dated</th>
        </tr>
        </thead>
        <tbody>

        </tbody>
      </table>
    </div>
<% end %>
<%= render 'partials/basic_content' %>