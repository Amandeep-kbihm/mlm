<%
  @heading = 'Enquiries'
  @sub_heading = 'All Enquiries'
  content_for :content do %>
    <div class="scrollbar">
      <table class="table table-striped table-hover" id="enquiries-table" data-source="<%= admin_enquiries_path(format: :json) %>">
        <thead>
        <tr>
          <th>ID</th>
          <th>Subject</th>
          <th>Query</th>
          <th>Mobile Number</th>
          <th>Status</th>
          <th>Date</th>
          <th>Actions</th>
        </tr>
        </thead>
        <tbody>

        </tbody>
      </table>
    </div>
<% end %>
<%= render 'partials/basic_content' %>